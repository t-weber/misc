func test_loop()
{
	int i;
	i = 0;

	loop i<16 do
		i = i+1;

	putint(i);
}


func test_args(int i, scalar j, scalar k)
{
	putint(i);
	putflt(j);
	putflt(k);
}


func test_vec()
{
	vec 3 v, w;
	v = [1., 2., 3.];
	v = w;
}


func test_str()
{
	str i, j;

	i = "Test 123";
	j = i;

	putstr(i);
	putstr(j);
}


func start()
{
	test_loop();
	test_args(12, 34., 56.);
	test_vec();
	test_str();
}
